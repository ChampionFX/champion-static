                                        <tbody>
                                        <tr>
                                            <td>1</td>
                                            <td>$0 - $10,000</td>
                                            <td>20%</td>
                                        </tr>
                                        <tr>
                                            <td>2</td>
                                            <td>$10,001 - $50,000</td>
                                            <td>25%</td>
                                        </tr>
                                        <tr>
                                            <td>3</td>
                                            <td>$50,000 - $100,000</td>
                                            <td>30%</td>
                                        </tr>
                                        <tr>
                                            <td>4</td>
                                            <td>$100,001 +</td>
                                            <td>35%</td>
                                        </tr>
                                        </tbody>
                                    </table>
